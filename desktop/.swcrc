{
	"sourceMaps": false,
	"module": {
		"type": "commonjs",
		"strictMode": true,
		"noInterop": false
	},
	"jsc": {
		"externalHelpers": false,
		"target": "es2024",
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": false,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": false,
			"react": {
				"throwIfNamespace": false,
				"useBuiltins": false,
				"pragma": "React.createElement",
				"pragmaFrag": "React.Fragment",
				"importSource": "react"
			}
		},
		"keepClassNames": true
	},
	"minify": true
}
