{
  "name": "nebulosa",
  "version": "0.1.0",
  "description": "The complete integrated solution for all of your astronomical imaging needs.",
  "author": {
    "name": "Tiago Melo",
    "email": "tiago.henrique.cco@gmail.com"
  },
  "homepage": "https://tiagohm.github.io/nebulosa",
  "license": "MIT",
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "copy:files": "node copyFiles.js",
    "start": "npm run copy:files && npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -c web --hmr",
    "build": "npm run copy:files && npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "web:build": "npm run build -- -c web-production",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "16.2.3",
    "@angular/cdk": "16.2.2",
    "@angular/common": "16.2.3",
    "@angular/compiler": "16.2.3",
    "@angular/core": "16.2.3",
    "@angular/forms": "16.2.3",
    "@angular/language-service": "16.2.3",
    "@angular/platform-browser": "16.2.3",
    "@angular/platform-browser-dynamic": "16.2.3",
    "@angular/router": "16.2.3",
    "chart.js": "4.4.0",
    "chartjs-plugin-zoom": "2.0.1",
    "cron": "2.4.3",
    "interactjs": "1.10.18",
    "leaflet": "1.9.4",
    "moment": "2.29.4",
    "panzoom": "9.4.3",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primeng": "16.3.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid": "9.0.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "16.0.1",
    "@angular-devkit/build-angular": "16.2.1",
    "@angular/cli": "16.2.1",
    "@angular/compiler-cli": "16.2.3",
    "@types/leaflet": "1.9.4",
    "@types/node": "20.5.7",
    "@types/uuid": "9.0.3",
    "electron": "26.1.0",
    "electron-builder": "24.6.3",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "wait-on": "7.0.1"
  },
  "engines": {
    "node": ">= 16.14.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 114"
  ],
  "copyFiles": [
    {
      "from": "src/shared/types.ts",
      "to": "app/types.ts"
    }
  ]
}
